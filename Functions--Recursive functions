long long fibonacci(long long n){           // While using recursion REMEMBER to ensure its iteration stoppage.
    if(n<=1)
        return n;
    return fibonacci(n-1)+fibonacci(n-2);
}
int main(){
    cout<<fibonacci(30)<<endl;
    return 0;
}
